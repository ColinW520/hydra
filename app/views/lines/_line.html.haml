%tr{id: "line_#{line.id}"}
  %td= line.name
  %td= number_to_phone line.number
  %td= local_time line.created_at, format: "%m/%d/%Y"

  %td.actions
    .btn-group
      = link_to line_path(line), class: 'btn btn-sm btn-info' do
        %i.fa.fa-eye
      = link_to edit_line_path(line), remote: true, class: 'btn btn-sm btn-warning' do
        %i.fa.fa-pencil

/ ['First Name', 'Last Name', 'Title', 'Mobile', 'City', 'State', 'Zip', 'Added']
