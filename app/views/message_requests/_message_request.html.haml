%tr{id: "message_request_#{message_request.id}"}
  %td= message_request.id
  %td= message_request.body
  %td= local_time message_request.created_at, format: "%m/%d/%Y"
  %td.actions
    .btn-group
      = link_to messages_path(message_request.filter_query.to_h), class: 'btn btn-sm btn-default' do
        %i.fa.fa-users
